import itertools

def solve_cryptarithmetic():
    # Letters used in the problem
    letters = 'SENDMORY'  # S,E,N,D,M,O,R,Y
    digits = '0123456789'

    # Try all possible digit assignments
    for perm in itertools.permutations(digits, len(letters)):
        mapping = dict(zip(letters, perm))

        # Leading letters cannot be 0
        if mapping['S'] == '0' or mapping['M'] == '0':
            continue

        # Form numbers from letters
        SEND = int(mapping['S'] + mapping['E'] + mapping['N'] + mapping['D'])
        MORE = int(mapping['M'] + mapping['O'] + mapping['R'] + mapping['E'])
        MONEY = int(mapping['M'] + mapping['O'] + mapping['N'] + mapping['E'] + mapping['Y'])

        # Check if solution is correct
        if SEND + MORE == MONEY:
            print("Solution Found:")
            print("SEND =", SEND)
            print("MORE =", MORE)
            print("MONEY =", MONEY)
            return

    print("No solution found")

# Driver code
solve_cryptarithmetic()
